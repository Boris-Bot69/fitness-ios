# For more information about the Appfile, see:
#     https://docs.fastlane.tools/advanced/#appfile

# The development bundle id
app_identifier "de.tum.in.www1.university.ios2122.mri.ipad"

# The team id for the university team
team_id "7PLLZ436SG"

# In the release build lane, we 1) change the app identifier to the
# TestFlight one and 2) specify the team id of the TestFlight team
for_lane :build do
    app_identifier "de.tum.in.www1.ios2122.mri.ipad"
    team_id "92SWX75A56"
end

for_lane :release do
    app_identifier "de.tum.in.www1.ios2122.mri.ipad"
    team_id "92SWX75A56"
end